{
  "name": "@siddhsuresh/generate",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --target es2022",
    "dev": "tsup src/index.ts --format esm,cjs --watch --target es2022",
    "generate": "node dist/index.js",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@siddhsuresh/tsconfig": "workspace:*",
    "argparse": "^2.0.1",
    "@octokit/rest": "^19.0.5",
    "eslint": "^7.32.0",
    "eslint-config-siddhsuresh": "workspace:*",
    "@types/node": "16.18.6",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
